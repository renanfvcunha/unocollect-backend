{
  "name": "unocollect",
  "version": "1.0.0",
  "description": "App to collect data from anywhere",
  "main": "server.js",
  "author": "R.C. Informatica",
  "license": "proprietary",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --ignore-watch node_modules src/server.ts",
    "start": "ts-node src/server.ts",
    "start:prod": "node build/server.js",
    "build": "tsc -p .",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -f src/database/config.ts",
    "typeorm:migrate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -f src/database/config.ts migration:run"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "sharp": "^0.26.1",
    "typeorm": "0.2.25"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/multer": "^1.4.4",
    "@types/node": "^8.0.29",
    "@types/sharp": "^0.26.0",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "eslint": "^7.4.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettierx": "^0.12.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.51",
    "typescript": "^3.9.6"
  }
}
